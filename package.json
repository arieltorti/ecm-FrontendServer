{
  "name": "ecm-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "watch": "vue-cli-service build --mode development --watch",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "katex": "^0.16.10",
    "plotly.js": "^1.54.2",
